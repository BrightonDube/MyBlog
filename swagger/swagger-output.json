{
  "openapi": "3.0.0",
  "info": {
    "title": "Node.js API with Express & MongoDB",
    "version": "1.0.0"
  },
  "components": {
    "securitySchemes": {
      "cookieAuth": {
        "type": "apiKey",
        "in": "cookie",
        "name": "connect.sid"
      }
    }
  },
  "security": [
    {
      "cookieAuth": []
    }
  ],
  "paths": {
    "/auth/google": {
      "get": {
        "summary": "Google OAuth login",
        "tags": ["Auth"]
      }
    },
    "/auth/google/callback": {
      "get": {
        "summary": "Google OAuth callback",
        "tags": ["Auth"]
      }
    },
    "/post": {
      "get": {
        "summary": "Get all posts",
        "tags": ["Post"]
      },
      "post": {
        "summary": "Create a post",
        "tags": ["Post"]
      }
    },
    "/post/{id}": {
      "put": {
        "summary": "Update a post",
        "tags": ["Post"]
      },
      "delete": {
        "summary": "Delete a post",
        "tags": ["Post"]
      }
    },
    "/user": {
      "get": {
        "summary": "Get all users",
        "tags": ["User"]
      }
    },
    "/user/{id}": {
      "delete": {
        "summary": "Delete a user",
        "tags": ["User"]
      }
    }
  },
  "tags": []
}
